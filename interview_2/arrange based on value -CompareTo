using System;
using System.Text;
using System.Collections.Generic;
using System.Linq;

class Mainn
{
    static void Duplicate_using_freq_arr(string s)
    {
      Dictionary<char,int>di=new Dictionary<char,int>();
      foreach(char c in s)
      {
          if(di.ContainsKey(c))
          {
              di[c]++;
          }
          else
          {
              di[c]=1;
          }
      }
      
      var list=di.ToList();
      list.Sort((a,b)=>b.Value.CompareTo(a.Value));
      
      foreach(var item in list)
      {
          Console.WriteLine(item.Key+" "+item.Value);
      }
      
    }
   static void Main()
   {
       string s=Console.ReadLine();
       Duplicate_using_freq_arr(s);
       
   }
}
