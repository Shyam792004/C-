
using System;
using System.Text;
using System.Collections.Generic;
using System.Linq;
class HelloWorld
{
    static void LongestWord(String s)
    {
       string [] arr=s.Split(' ');
       int max=int.MinValue;
       string ans=arr[0];
       for(int i=0;i<arr.Length;i++)
       {
           if(max<arr[i].Length)
           {
               max=arr[i].Length;
               ans=arr[i];
           }
       }
       Console.Write(ans);
       
    }
    
   static void Main() 
   {

      string s=Console.ReadLine();
      LongestWord(s);
      
   }
}
