
using System;
using System.Linq;
using System.Collections.Generic;

class HelloWorld {
  static void Main() {
      int n=int.Parse(Console.ReadLine());
      int rev=0;
      while(n>0)
      {
          rev=rev*10+n%10;
          n=n/10;
      }
      Console.WriteLine(rev);
      
      //for 0s
      string s=Console.ReadLine();
      char [] c=s.ToCharArray();
      Array.Reverse(c);
      Console.WriteLine(new String(c));
  }
  
}
