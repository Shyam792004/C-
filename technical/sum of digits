
using System;
using System.Linq;
using System.Collections.Generic;

class HelloWorld {
    static void sum(int a)
    {
        int sum=0;
      while(a>0)
      {
         sum+= a%10;
         a=a/10;
      }
      Console.WriteLine(sum);
    }
    
  static void Main() {
      int a=int.Parse(Console.ReadLine());
      sum(a);
      
      
      
  }
  
}
